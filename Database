create table matchentity
( MatchID number(3) PRIMARY KEY,
Team1 varchar2(15),
Team2 varchar2(15),
date1 varchar2(15),
location varchar2(10)); 


insert into matchentity(MatchID,Team1,Team2,date1,location) values (1, 'India', 'England', '1-Aug-2018','Edgbaston');
